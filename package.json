{
  "name": "leod",
  "version": "0.1.1",
  "description": "React components library",
  "private": false,
  "main": "dist/index.js",
  "author": "leoh",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "Component",
    "UI",
    "React"
  ],
  "homepage": "https://github.com/leohai/leod",
  "repository": {
    "type": "git",
    "url": "https://github.com/leohai/leod"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "clean": "rimraf ./dist",
    "build": "yarn clean && yarn build-tsc && yarn build-sass",
    "lint": "eslint --ext .ts,.js,.tsx src/ --max-warnings 5",
    "test": "react-scripts test",
    "test:nowatch": "cross-env CI=true yarn test",
    "eject": "react-scripts eject",
    "build-tsc": "tsc -p tsconfig.build.json",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "build-sass": "node-sass ./src/styles/index.scss ./dist/index.css",
    "prepublishOnly": "yarn lint && yarn test:nowatch && yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test:nowatch "
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.5",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-docs": "^5.3.18",
    "@storybook/addon-links": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/preset-create-react-app": "^2.1.2",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/classnames": "^2.2.10",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-transition-group": "^4.2.4",
    "cross-env": "^7.0.2",
    "husky": "^4.2.5",
    "node-sass": "^7.0.0",
    "react": "^16.13.0",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.0",
    "react-scripts": "3.4.0",
    "rimraf": "^3.0.2",
    "typescript": "~3.7.2"
  }
}